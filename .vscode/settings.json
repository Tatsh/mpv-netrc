{
  "Lua.workspace.checkThirdParty": false,
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.insertSpaces": true,
  "editor.rulers": [100],
  "editor.tabSize": 2,
  "files.associations": {
    "*.rockspec": "lua",
    ".busted": "lua",
    ".luacheckrc": "lua",
    ".luacov": "lua"
  }
}
